  title = {Square {{Moir}}{\textbackslash}'e {{Superlattices}} in {{Twisted Two-Dimensional Halide Perovskites}}},
  author = {Zhang, Shuchen and Jin, Linrui and Lu, Yuan and Zhang, Linghai and Yang, Jiaqi and Zhao, Qiuchen and Sun, Dewei and Thompson, Joshua J. P. and Yuan, Biao and Ma, Ke and Akriti and Park, Jee Yung and Lee, Yoon Ho and Wei, Zitang and Finkenauer, Blake P. and Blach, Daria D. and Kumar, Sarath and Peng, Hailin and {Mannodi-Kanakkithodi}, Arun and Yu, Yi and Malic, Ermin and Lu, Gang and Dou, Letian and Huang, Libai},
  year = {2023},
