  langid = {english},
  keywords = {Computational science,Materials science,Mathematics and computing,Physics},
  annotation = {PtychoPINN},
  file = {/Users/swim/Zotero/storage/B5K3IAY5/Hoidn et al. - 2023 - Physics constrained unsupervised deep learning for.pdf}
